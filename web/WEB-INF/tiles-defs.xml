<?xml version="1.0" encoding="UTF-8" ?>

 <!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
       "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">


<!--
     This is a blank Tiles definition file with a commented example.
-->

<tiles-definitions>

    
    <definition name="base" path="/Plantilla/plantilla.jsp">
        <put name="banner" value="/Plantilla/banner.jsp"/>
        <put name="sidebarR" value="/Plantilla/sidebarR.jsp"/>
        <put name="body" value="/Plantilla/body.jsp"/>
        <put name="footer" value="/Plantilla/footer.jsp"/>
    </definition>
    
    <definition name="baseAdmin" extends="base"> <!-- BASE PARA ADMIN -->
        <put name="sidebarL" value="/Plantilla/sidebarL.jsp"/>
    </definition>
    
    <definition name="InicioAdmin" extends="baseAdmin"> <!-- Vista administrador -->
        <put name="body" value="/Inicio.jsp"/>
    </definition>
    
    <!--
    -
    Para cuando le das click a Tipo de Presupuesto (para resumir esta frase
    se utilizaran las siglas: TDP)
    -
    -->
    <definition name="baseAdminTDP" extends="base"> <!-- Base admin para TDP -->
        <put name="sidebarL" value="/Plantilla/sidebarLTDP.jsp"/>
    </definition>
    
    <definition name="consultarTDP" extends="baseAdminTDP"> <!-- Consulta de TDP -->
        <put name="body" value="/TipoDePresupuesto/consultar_tipo_de_presupuesto.jsp"/>
    </definition>
    
    <definition name="consultarTDPDescendente" extends="baseAdminTDP"> <!-- Consulta de TDP por codigo Descendente -->
        <put name="body" value="/TipoDePresupuesto/consultar_tipo_de_presupuesto_descendente.jsp"/>
    </definition>        
            
    <definition name="consultarTDPmonto" extends="baseAdminTDP"> <!-- Consulta de TDP por monto -->
        <put name="body" value="/TipoDePresupuesto/consultar_tipo_de_presupuesto_pormonto.jsp"/>
    </definition>    
       
    <definition name="consultarTDPmontoDescendente" extends="baseAdminTDP"> <!-- Consulta de TDP por monto descendente -->
        <put name="body" value="/TipoDePresupuesto/consultar_tipo_de_presupuesto_pormonto_descendente.jsp"/>
    </definition>
                                                  
    <definition name="consultarTDPtipo" extends="baseAdminTDP"> <!-- Consulta de TDP por tipo-->
        <put name="body" value="/TipoDePresupuesto/consultar_tipo_de_presupuesto_portipo.jsp"/>
    </definition>
    
    <definition name="consultarTDPtipoDescendente" extends="baseAdminTDP"> <!-- Consulta de TDP por tipo descendente -->
        <put name="body" value="/TipoDePresupuesto/consultar_tipo_de_presupuesto_portipo_descendente.jsp"/>
    </definition>    
    
    <definition name="agregarTDP" extends="baseAdminTDP"> <!-- Agregacion de TDP -->
        <put name="body" value="/TipoDePresupuesto/agregar_tipo_de_presupuesto.jsp"/>
    </definition>
           
    <definition name="nuevosDatosDeTDP" extends="baseAdminTDP"> <!-- Datos para modificar un TDP y caer en la vista principal -->
        <put name="body" value="/TipoDePresupuesto/listar_tipo_de_modificar.jsp"/>
    </definition>
    
    <definition name="nuevosDatosDeTDPportipo" extends="baseAdminTDP"> <!-- Datos para modificar un TDP y caer en la vista por tipo -->
        <put name="body" value="/TipoDePresupuesto/listar_tipo_de_modificar_tipo.jsp"/>
    </definition>
    
    <definition name="nuevosDatosDeTDPpormonto" extends="baseAdminTDP"> <!-- Datos para modificar un TDP y caer en la vista por monto -->
        <put name="body" value="/TipoDePresupuesto/listar_tipo_de_modificar_monto.jsp"/>
    </definition>        
        
    <definition name="buscarTDP" extends="baseAdminTDP"> <!-- Modificacion de TDP exitosa -->
        <put name="body" value="/TipoDePresupuesto/buscar_tipo_de_presupuesto.jsp"/>
    </definition>
    
    <!--
    -
    Para cuando le das click a Presupuesto 
    -
    -->
    <definition name="baseAdminPresupuesto" extends="base"> <!-- Base admin para presupuesto -->
        <put name="sidebarL" value="/Plantilla/sidebarLPresupuesto.jsp"/>
    </definition>
    
    <definition name="asignarPresupuesto" extends="baseAdminPresupuesto"> <!-- Asignacion de los Presupuestos -->
        <put name="body" value="/Presupuesto/asignar_presupuesto.jsp"/>
    </definition>   
    
    <definition name="consultarPresupuesto" extends="baseAdminPresupuesto"> <!-- Consulta de los Presupuestos -->
        <put name="body" value="/Presupuesto/consultar_presupuesto.jsp"/>
    </definition>       
    
    <definition name="consultarPresupuestoTotales" extends="baseAdminPresupuesto"> <!-- Consulta de los totales de los Presupuestos -->
        <put name="body" value="/Presupuesto/consultar_presupuesto_totales.jsp"/>
    </definition>
                        
    <definition name="asignarPresupuestoEquitativo" extends="baseAdminPresupuesto"> <!-- Asignacion de los Presupuestos Equitativamente -->
        <put name="body" value="/Presupuesto/asignar_presupuesto_equitativo.jsp"/>
    </definition>
        
    <definition name="buscarPresupuesto" extends="baseAdminPresupuesto"> <!-- Busqueda de los presupuestos  -->
        <put name="body" value="/Presupuesto/buscar_presupuesto.jsp"/>
    </definition>
    
    <definition name="nuevosDatosDePresupuesto" extends="baseAdminPresupuesto"> <!-- Nuevos datos para la modificacion de un presupuesto -->
        <put name="body" value="/Presupuesto/listar_modificar_presupuesto.jsp"/>
    </definition>
    
    <!--Para consulta de los tipos de laboratorios existentes-->
    <definition name="consultarLaboratorio" extends="baseAdmin"> <!-- Consulta de los laboratorios existentes -->
        <put name="body" value="/Laboratorio/consulta_laboratorios.jsp"/>
    </definition>

</tiles-definitions>
